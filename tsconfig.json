      "@GTU/Exam": ["repos/testify/src/exam"],
      "@GTU/Exam/*": ["repos/testify/src/exam/*"],
      "@GTU/Utils": ["repos/testify/src/utils"],
      "@GTU/Utils/*": ["repos/testify/src/utils/*"],
      "@GTU/Steps": ["repos/testify/src/steps"],
      "@GTU/Steps/*": ["repos/testify/src/steps/*"],
      "@GTU/Parkin": ["repos/testify/src/parkin"],
      "@GTU/Parkin/*": ["repos/testify/src/parkin/*"],
      "@GTU/Support": ["repos/testify/src/support"],
      "@GTU/Support/*": ["repos/testify/src/support/*"],
      "@GTU/Reports": ["repos/testify/src/reports"],
      "@GTU/Reports/*": ["repos/testify/src/reports/*"],
      "@GTU/PlaywrightEnv": ["repos/testify/src/playwright/playwrightTestEnv"],
