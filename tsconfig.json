      "@GEX/constants/*": ["repos/exam/src/constants/*"],
      "@GEX/environment": ["repos/exam/src/environment"],
      "@GEX/environment/*": ["repos/exam/src/environment/*"],
      "@GEX/runner": ["repos/exam/src/runner"],
      "@GEX/runner/*": ["repos/exam/src/runner/*"],
