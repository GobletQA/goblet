      "@gobletqa/git": ["repos/git/src"],
      "@gobletqa/git/*": ["repos/git/src/*"],

      "@gobletqa/latent": ["repos/latent/src"],
      "@gobletqa/latent/*": ["repos/latent/src/*"],

      "@gobletqa/logger": ["repos/logger/src"],
      "@gobletqa/logger/*": ["repos/logger/src/*"],

      "@gobletqa/monaco": ["repos/monaco/src"],
      "@gobletqa/monaco/*": ["repos/monaco/src/*"],

      "@gobletqa/race": ["repos/race/src"],
      "@gobletqa/race/*": ["repos/race/src/*"],

      "@gobletqa/repo": ["repos/repo/src"],
      "@gobletqa/repo/*": ["repos/repo/src/*"],

      "@gobletqa/screencast": [ "repos/screencast/src" ],
      "@gobletqa/screencast/*": [ "repos/screencast/src/*" ],

      "@gobletqa/shared": [ "repos/shared/src" ],
      "@gobletqa/shared/*": [ "repos/shared/src/*" ],

      "@gobletqa/testify": [ "repos/testify/src" ],
      "@gobletqa/testify/*": [ "repos/testify/src/*" ],

      "@gobletqa/workflows": ["repos/workflows/src"],
      "@gobletqa/workflows/*": ["repos/workflows/src/*"],

      "@gobletqa/root/gobletRoot": ["gobletRoot.js"],
      "@gobletqa/root": ["./*"]
    }
  },
  "exclude": [
    "tasks/goblet.ts",
    "node_modules",
    "bundle",
    ".git",
    ".husky",
    ".firebase",
    "goblet",
    "certs",
    "repos/**/dist",
    "repos/**/build",
    "repos/**/node_modules",
    "**/.tsup",
