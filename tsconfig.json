{
  "parserOptions": {
    "extraFileExtensions": ["json"]
  },
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "outDir": "./dist",
    "noEmit": true,
    "allowJs": true,
    "sourceMap": true,
    "declaration": false,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {

      "@GBE/configs": ["repos/backend/configs"],
      "@GBE/configs/*": ["repos/backend/configs/*"],
      "@GBE/Templates": ["repos/backend/templates"],
      "@GBE/Templates/*": ["repos/backend/templates/*"],
      "@GBE": ["repos/backend/src"],
      "@GBE/*": ["repos/backend/src/*"],
      "@GBE/Root": ["repos/backend/"],


      "@GBB/scripts": ["repos/browser/scripts"],
      "@GBB/scripts/*": ["repos/browser/scripts/*"],
      "@GBB/types": ["repos/browser/src/types"],
      "@GBB/types/*": ["repos/browser/src/types/*"],
      "@GBB/utils": ["repos/browser/src/utils"],
      "@GBB/utils/*": ["repos/browser/src/utils/*"],
