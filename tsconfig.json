  "exclude": [
    "tasks/goblet.ts",
    "node_modules",
    "bundle",
    ".git",
    ".husky",
    ".firebase",
    "goblet",
    "certs",
    "repos/**/dist",
    "repos/**/build",
    "repos/**/node_modules",
    "**/.tsup",
    "docs",
    "logs",
    "scripts",
    "temp"
