      "@GGT/types": ["repos/git/src/types"],
      "@GGT/types/*": ["repos/git/src/types/*"],
      "@GGT/utils": ["repos/git/src/utils"],
      "@GGT/utils/*": ["repos/git/src/utils/*"],
      "@GGT": ["repos/git/src"],
      "@GGT/*": ["repos/git/src/*"],

      "@GJK/constants": ["repos/joker/src/constants"],
