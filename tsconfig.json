      "@gobletqa/backend/*": ["repos/backend/src/*"],

      "@gobletqa/browser": ["repos/browser/src"],
      "@gobletqa/browser/*": ["repos/browser/src/*"],

      "@gobletqa/configs": ["configs"],
      "@gobletqa/configs/*": ["configs/*"],

      "@gobletqa/components": ["repos/components/src"],
      "@gobletqa/components/*": ["repos/components/src/*"],

      "@gobletqa/conductor": ["repos/conductor/src"],
      "@gobletqa/conductor/*": ["repos/conductor/src/*"],
      
      "@gobletqa/environment": ["repos/environment/src"],
      "@gobletqa/environment/*": ["repos/environment/src/*"],

      "@gobletqa/exam": ["repos/exam/src"],
      "@gobletqa/exam/*": ["repos/exam/src/*"],

      "@gobletqa/goblet": ["repos/goblet/src"],
