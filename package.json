{
  "name": "@gobletqa/goblet",
  "version": "0.0.0",
  "description": "Web application testing framework",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "author": "Lance Tipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "repos/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:docker": "turbo run build:docker",
    "build:fe": "node tasks/entry.js deploy build -- frontend=true",
    "build:all": "turbo run build",
    "bundle": "node scripts/bundle.js",
    "bundle:compile": "yarn bundle && yarn compile",
    "certs": "/bin/bash scripts/shell/generate.sh $@",
    "clean": "turbo run clean",
    "clean:nm": "rimraf -rf ./node_modules; turbo run clean:nm",
    "clean:full": "yarn clean; yarn clean:nm; yarn setup",
    "cz": "cz",
    "compile": "node scripts/compile.js",
    "dev": "node tasks/entry.js devspace",
    "doc": "node tasks/entry.js docker",
    "format": "yarn format:pretty && yarn format:lint",
    "format:all": "yarn format && turbo run format",
    "format:lint": "yarn lint .",
    "format:pretty": "yarn pretty .",
    "kube": "node tasks/entry.js kube",
    "imgs": "node tasks/entry.js docker images",
    "lint": "eslint --color --cache --fix --ignore-path .root.ignore --config ./configs/eslintrc.config.js",
    "make": "GB_ROOT_DIR=$(pwd) ./scripts/shell/init/init.sh",
    "mkcert": "GB_ROOT_DIR=$(pwd) ./scripts/shell/mkcert/mkcert.sh",
    "pretty": "prettier --ignore-path .root.ignore --config ./configs/prettier.config.js --write",
    "sc": "node tasks/entry.js screencast",
    "screencast": "node tasks/entry.js screencast",
    "task": "node tasks/entry.js",
    "cp:tracer": "node scripts/js/cpTracer.js",
    "pm2": "pm2 start configs/app.pm2.config.js",
    "setup": "yarn install && yarn cp:tracer",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "validate": "turbo run validate"
  },
  "resolutions": {
    "@jest/transform": "29.2.0",
    "@keg-hub/args-parse": "9.0.0",
    "@keg-hub/ask-it": "1.1.2",
    "@keg-hub/cli-utils": "0.7.0",
    "@keg-hub/jsutils": "9.4.0",
    "@keg-hub/parse-config": "2.1.0",
    "@keg-hub/spawn-cmd": "0.3.2",
    "@ltipton/parkin": "1.4.0",
    "@monaco-editor/react": "4.3.1",
    "jest": "29.2.0",
    "nodemon": "2.0.19",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@keg-hub/args-parse": "9.0.0",
    "@keg-hub/ask-it": "1.1.2",
    "@keg-hub/cli-utils": "0.7.0",
    "@keg-hub/jsutils": "9.4.0",
    "@keg-hub/parse-config": "2.1.0",
    "@keg-hub/spawn-cmd": "0.3.2",
    "@ltipton/parkin": "1.4.0",
    "cross-env": "7.0.3",
    "esbuild-register": "3.3.3",
    "@types/node": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.15.3",
    "esbuild-plugin-path-alias": "1.0.6",
    "eslint": "8.7.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "module-alias": "2.2.2",
    "nodemon": "2.0.19",
    "playwright": "1.28.0",
    "prettier": "2.5.1",
    "semver": "7.3.7",
    "turbo": "1.4.5",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*": [
      "yarn pretty",
      "yarn lint"
    ]
  }
}
