{
  "name": "@gobletqa/goblet",
  "version": "0.0.0",
  "packageManager": "yarn@1.22.19",
  "description": "Web application testing framework",
  "main": "index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "author": "Lance Tipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "repos/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:docker": "turbo run build:docker",
    "build:fe": "node -r esbuild-register tasks/entry.ts deploy build -- frontend=true",
    "build:all": "turbo run build",
    "bundle": "node scripts/bundle.js",
    "bundle:compile": "yarn bundle && yarn compile",
    "certs": "/bin/bash scripts/shell/generate.sh $@",
    "clean": "turbo run clean",
    "clean:nm": "rimraf -rf ./node_modules; turbo run clean:nm",
    "clean:full": "yarn clean; yarn clean:nm; yarn setup",
    "cz": "cz",
    "compile": "node scripts/compile.js",
    "dev": "node -r esbuild-register tasks/entry.ts devspace",
    "doc": "node -r esbuild-register tasks/entry.ts docker",
    "deploy": "node -r esbuild-register tasks/entry.ts deploy",
    "format": "yarn format:pretty && yarn format:lint",
    "format:all": "yarn format && turbo run format",
    "format:lint": "yarn lint .",
    "format:pretty": "yarn pretty .",
    "imgs": "node -r esbuild-register tasks/entry.ts docker images",
    "kube": "node -r esbuild-register tasks/entry.ts kube",
    "json:rm": "dot-json package.json devDependencies -d",
    "json:repo-rm": "turbo run json:rm",
    "lint": "eslint --color --cache --fix --ignore-path .root.ignore --config ./configs/eslintrc.config.js",
    "make": "GB_ROOT_DIR=$(pwd) ./scripts/shell/init/init.sh",
    "mkcert": "GB_ROOT_DIR=$(pwd) ./scripts/shell/mkcert/mkcert.sh",
    "pretty": "prettier --ignore-path .root.ignore --config ./configs/prettier.config.js --write",
    "sc": "node -r esbuild-register tasks/entry.ts screencast",
    "screencast": "node -r esbuild-register tasks/entry.ts screencast",
    "ts": "node -r esbuild-register tasks/entry.ts",
    "task": "node -r esbuild-register tasks/entry.ts",
    "cp:tracer": "node scripts/js/cpTracer.js",
    "pm2": "pm2 start configs/app.pm2.config.js",
    "setup": "yarn install && yarn cp:tracer",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "validate": "turbo run validate"
  },
  "resolutions": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@jest/transform": "29.2.0",
    "@jest/create-cache-key-function": "29.2.0",
    "@keg-hub/args-parse": ">=10.0.0",
    "@keg-hub/cli-utils": "0.8.0-alpha",
    "@keg-hub/jsutils": ">=9.6.1",
    "@keg-hub/parse-config": ">=2.1.0",
    "@keg-hub/spawn-cmd": ">=0.3.2",
    "@kubernetes/client-node": "0.17.1",
    "@ltipton/parkin": "3.0.0-beta-8",
    "@monaco-editor/react": "4.3.1",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.2",
    "@mui/x-date-pickers": "5.0.12",
    "@mui/lab": "5.0.0-alpha.114",
    "axios": "0.27.2",
    "@ltipton/esdev": "0.1.3",
    "esbuild": "0.17.6",
    "esbuild-plugin-path-alias": "1.0.7",
    "esbuild-register": "3.4.2",
    "esbuild-jest": "0.5.0",
    "expect": "29.3.1",
    "express": "4.17.3",
    "http-proxy-middleware": "2.0.4",
    "jest": "29.2.0",
    "jest-jasmine2": "29.2.0",
    "jsonwebtoken": "8.5.1",
    "module-alias": "2.2.2",
    "nodemon": "2.0.19",
    "playwright": "1.27.0",
    "react-selecto": "1.22.0",
    "socket.io": "4.5.4",
    "source-map-support": "0.5.21",
    "serve": "14.1.2",
    "typescript": "4.8.4",
    "@vitejs/plugin-react": "2.1.0",
    "vite": "3.1.4",
    "vite-plugin-svgr": "2.2.1",
    "vite-plugin-svgr-component": "1.0.1",
    "vite-tsconfig-paths": "3.5.0"
  },
  "dependencies": {
    "@keg-hub/args-parse": ">=10.0.0",
    "@keg-hub/cli-utils": "0.8.0-alpha1",
    "@keg-hub/jsutils": ">=9.6.1",
    "@keg-hub/parse-config": ">=2.1.0",
    "@ltipton/parkin": "3.0.0-beta-8",
    "@ltipton/esdev": "*",
    "esbuild": "*",
    "esbuild-plugin-path-alias": "*",
    "esbuild-register": "*",
    "esbuild-jest": "*",
    "module-alias": "*",
    "nodemon": "*",
    "turbo": "1.4.5"
  },
  "devDependencies": {
    "@keg-hub/spawn-cmd": ">=0.3.2",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@types/node": "17.0.13",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dot-json": "1.2.2",
    "eslint": "8.7.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "playwright": "*",
    "prettier": "2.5.1",
    "semver": "7.3.7",
    "typescript": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*": [
      "yarn pretty",
      "yarn lint"
    ]
  }
}
