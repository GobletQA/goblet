{
  "name": "@gobletqa/goblet",
  "version": "0.0.0",
  "description": "Web application testing framework",
  "main": "index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "author": "Lance Tipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "repos/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:docker": "pnpm -r build:docker",
    "build:fe": "node -r esbuild-register tasks/entry.ts deploy build -- frontend=true",
    "bundle": "node scripts/bundle.js",
    "bundle:compile": "pnpm bundle && pnpm compile",
    "certs": "/bin/bash scripts/shell/generate.sh $@",
    "clean": "pnpm -r clean",
    "clean:nm": "rimraf -rf ./node_modules; pnpm -r clean:nm",
    "clean:full": "pnpm clean; pnpm clean:nm; pnpm setup",
    "cz": "cz",
    "compile": "node scripts/compile.js",
    "dev": "node -r esbuild-register tasks/entry.ts devspace",
    "doc": "node -r esbuild-register tasks/entry.ts docker",
    "dep": "node -r esbuild-register tasks/entry.ts deploy",
    "format": "pnpm format:pretty && pnpm format:lint",
    "format:all": "pnpm format && pnpm -r format",
    "format:lint": "pnpm lint .",
    "format:pretty": "pnpm pretty .",
    "imgs": "node -r esbuild-register tasks/entry.ts docker images",
    "kube": "node -r esbuild-register tasks/entry.ts kube",
    "json:rm": "dot-json package.json devDependencies -d",
    "json:rm-all": "pnpm json:rm && pnpm -r json:rm",
    "lint": "eslint --color --cache --fix --ignore-path .root.ignore --config ./configs/eslintrc.config.js",
    "make": "GB_ROOT_DIR=$(pwd) ./scripts/shell/init/init.sh",
    "mkcert": "GB_ROOT_DIR=$(pwd) ./scripts/shell/mkcert/mkcert.sh",
    "pretty": "prettier --ignore-path .root.ignore --config ./configs/prettier.config.js --write",
    "sc": "node -r esbuild-register tasks/entry.ts screencast",
    "screencast": "node -r esbuild-register tasks/entry.ts screencast",
    "ts": "node -r esbuild-register tasks/entry.ts",
    "task": "node -r esbuild-register tasks/entry.ts",
    "cp:tracer": "node scripts/js/cpTracer.js",
    "pm2": "pm2 start configs/app.pm2.config.js",
    "setup": "pnpm install && pnpm cp:tracer",
    "test": "pnpm -r test",
    "test:ci": "pnpm -r test:ci",
    "validate": "pnpm -r validate"
  },
  "pnpm": {
    "overrides": {
      "@emotion/react": "11.11.0"
    },
    "peerDependencyRules": {
      "ignoreMissing": ["dockerode", "react"],
      "neverBuiltDependencies": ["ssh2"]
    }
  },
  "dependencies": {
    "@keg-hub/args-parse": ">=10.0.0",
    "@keg-hub/cli-utils": "0.8.0-alpha1",
    "@keg-hub/jsutils": ">=9.6.1",
    "@keg-hub/parse-config": ">=2.1.0",
    "@ltipton/parkin": "4.1.0",
    "@ltipton/esdev": "0.1.3",
    "esbuild": "0.17.17",
    "esbuild-plugin-path-alias": "1.0.7",
    "esbuild-register": "3.4.2",
    "esbuild-jest": "0.5.0",
    "module-alias": "2.2.2",
    "nodemon": "2.0.22"
  },
  "devDependencies": {
    "@keg-hub/spawn-cmd": ">=0.3.2",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@types/node": "18.15.13",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dot-json": "1.2.2",
    "eslint": "8.7.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "playwright": "1.35.0",
    "prettier": "2.5.1",
    "semver": "7.3.7",
    "typescript": "5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*": [
      "pnpm pretty",
      "pnpm lint"
    ]
  }
}
