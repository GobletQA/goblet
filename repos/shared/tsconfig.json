{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@Template": ["src/templates"],
      "@FileSys": ["src/libs/fileSys"],
      "@Features": ["src/libs/features"],
      "@Definitions": ["src/libs/definitions"],
      "@Libs": ["src/libs"],
      "@Utils": ["src/utils"],
      "@Express": ["src/express"],
      "@Models": ["src/models"],
      "@App": ["src/express/app.js"],
      "@Paths": ["src/utils/paths.js"],
      "@Middleware": ["src/middleware"],
      "@Router": ["src/express/appRouter.js"],
      "@Config": ["src/utils/getGobletConfig.js"],
      "@AsyncWrap": ["src/express/asyncWrap.js"],
      "@Repo": ["src/repo"],
      "@GSH": ["src"],
      "@GSHRoot": ["."],
      "@gobletqa/test-utils": ["../testUtils/src"],
      "@gobletqa/test-utils/*": ["../testUtils/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
