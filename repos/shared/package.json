{
  "name": "@gobletqa/shared",
  "version": "0.0.0",
  "description": "Goblet Shared code",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "exit 0",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "format": "pnpm lint && pnpm pretty",
    "json:rm": "dot-json package.json devDependencies -d",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w"
  },
  "dependencies": {
    "@gobletqa/latent": "workspace:../latent",
    "@keg-hub/cli-utils": ">=0.8.0-alpha1",
    "@keg-hub/jsutils": ">=9.6.1",
    "@keg-hub/parse-config": ">=2.1.0",
    "@ltipton/parkin": "5.0.0",
    "cookie-session": "2.0.0",
    "cors": "2.8.5",
    "express": "4.18.2",
    "http-proxy-middleware": "2.0.6",
    "express-blacklist": "1.0.3",
    "express-defend": "1.0.9",
    "express-jwt": "8.3.0",
    "express-winston": "4.2.0",
    "fs-extra": "10.1.0",
    "glob": "10.3.3",
    "is-secret": "1.2.1",
    "mime": "3.0.0",
    "module-alias": "2.2.2",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@types/dockerode": "3.3.9",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.17",
    "@types/jest": "29.1.2",
    "@types/node": "18.15.13",
    "esbuild": "0.17.18",
    "esbuild-register": "3.4.2",
    "express": "4.18.2",
    "http-proxy-middleware": "2.0.6",
    "jest": "29.5.0",
    "socket.io": "4.6.1"
  }
}
