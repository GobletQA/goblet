{
  "name": "@gobletqa/shared",
  "version": "0.0.0",
  "description": "Goblet Shared code",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "exit 0",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:install": "yarn clean:full; yarn install",
    "clean:full": "yarn clean; yarn clean:nm; yarn install",
    "clean:nm": "rm -rf node_modules",
    "format": "yarn lint && yarn pretty",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w"
  },
  "resolutions": {
    "@keg-hub/cli-utils": "0.7.0",
    "@keg-hub/jsutils": "9.4.0",
    "@keg-hub/parse-config": "2.1.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "express": "4.X",
    "http-proxy-middleware": "2.0.4"
  },
  "dependencies": {
    "@keg-hub/cli-utils": "*",
    "@keg-hub/jsutils": "*",
    "@keg-hub/parse-config": "*",
    "@ltipton/parkin": "*",
    "cookie-session": "2.0.0",
    "cors": "2.8.5",
    "express-blacklist": "1.0.3",
    "express-defend": "1.0.9",
    "express-jwt": "6.1.1",
    "express-winston": "4.2.0",
    "glob": "8.0.3",
    "fs-extra": "10.1.0",
    "is-secret": "1.2.1",
    "mime": "3.0.0",
    "module-alias": "2.2.2",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@gobletqa/workflows": "*",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.9",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.0",
    "esbuild": "0.15.3",
    "esbuild-plugin-path-alias": "1.0.6",
    "jest": "29.2.0",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
