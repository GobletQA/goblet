{
  "name": "@gobletqa/kind",
  "version": "0.0.0",
  "description": "Api Access to kubectl",
  "main": "index.js",
  "author": "LanceTipton",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn build:docker",
    "build:docker": "node configs/build.config.js",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:install": "yarn clean:full; yarn install",
    "clean:full": "yarn clean; yarn clean:nm; yarn install",
    "clean:nm": "rm -rf node_modules",
    "format": "yarn lint && yarn pretty",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "start": "DEBUG=http-proxy-middleware* DEV_BUILD=1 node --max-http-header-size=262144 configs/build.config.js",
    "serve": "node dist/index.js",
    "validate": "tsc --noEmit"
  },
  "resolutions": {
    "@keg-hub/cli-utils": "0.7.0",
    "@keg-hub/jsutils": "9.3.5",
    "@keg-hub/parse-config": "2.1.0",
    "nodemon": "2.0.19",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@keg-hub/cli-utils": "*",
    "@keg-hub/jsutils": "*",
    "@keg-hub/parse-config": "*",
    "@kubernetes/client-node": "0.17.1",
    "axios": "0.24.0",
    "esbuild": "0.15.3",
    "esbuild-plugin-path-alias": "1.0.6",
    "esbuild-register": "3.3.3",
    "http-proxy-middleware": "2.0.4",
    "module-alias": "2.2.2",
    "source-map-support": "0.5.21",
    "express": "4.17.3"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/express": "4.17.9",
    "@types/jest": "29.1.2",
    "@types/node": "17.0.13",
    "esbuild": "0.15.3",
    "esbuild-plugin-path-alias": "1.0.6",
    "jest": "29.2.0",
    "nodemon": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
