{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./src/types",
      "../shared/src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@GKD/Configs": ["configs"],
      "@GKD/Configs/*": ["configs/*"],
      "@GKD/Constants": ["src/constants"],
      "@GKD/Constants/*": ["src/constants/*"],
      "@GKD/Kubectl": ["src/libs/kubectl"],
      "@GKD/Kubectl/*": ["src/libs/kubectl/*"],
      "@GKD/Libs": ["src/libs"],
      "@GKD/Libs/*": ["src/libs/*"],
      "@GKD/Middleware": ["src/middleware"],
      "@GKD/Middleware/*": ["src/middleware/*"],
      "@GKD/Types": ["src/types"],
      "@GKD/Types/*": ["src/types/*"],
      "@GKD/Root": ["./"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@gobletqa/shared": ["../shared/src"],
      "@GLogs/*": ["../../logs/*"],
      "@GRepos/*": ["../../repos/*"],
      "@GConfigs/*": ["../../configs/*"],
      "@GEnvironment": ["../../scripts/environment"]
    }
  },
  "include": [
    "src",
    "configs"
, "scripts/kubectl"  ],
  "exclude": ["node_modules", "dist"]
}