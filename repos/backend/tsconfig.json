{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "typeRoots": [
      "./src/types",
      "../shared/src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@GBE/Configs": ["configs"],
      "@GBE/Configs/*": ["configs/*"],
      "@GBE/Templates": ["templates"],
      "@GBE/Templates/*": ["templates/*"],
      "@GBE": ["src"],
      "@GBE/*": ["src/*"],
      "@GBE/Root": ["./"],
      "@gobletqa/backend": ["src"],
      "@gobletqa/backend/*": ["src/*"],
      "@gobletqa/conductor": ["../conductor/src"],
      "@gobletqa/conductor/*": ["../conductor/src/*"],
      "@gobletqa/screencast": ["../screencast/src"],
      "@gobletqa/screencast/*": ["../screencast/src/*"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@gobletqa/shared": ["../shared/src"],
      "@GLogs/*": ["../../logs/*"],
      "@GRepos/*": ["../../repos/*"],
      "@GConfigs/*": ["../../configs/*"],
      "@GEnvironment": ["../../scripts/environment"]
    }
  },
  "include": [
    "src",
    "configs",
    "scripts",
    "../shared",
    "../testUtils",
    "../workflows"
  ],
  "exclude": ["node_modules", "dist"]
}