{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@GBE/Configs": ["configs"],
      "@GBE/Configs/*": ["configs/*"],
      "@GBE": ["src"],
      "@GBE/*": ["src/*"],
      "@GBE/Root": ["./"],
      "@gobletqa/backend": ["src"],
      "@gobletqa/backend/*": ["src/*"],
      "@gobletqa/conductor": ["../conductor/src"],
      "@gobletqa/conductor/*": ["../conductor/src/*"],
      "@gobletqa/screencast": ["../screencast/src"],
      "@gobletqa/screencast/*": ["../screencast/src/*"],
      "@gobletqa/shared": ["../shared/src"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@GLogs/*": ["../../logs/*"],
      "@GRepos/*": ["../../repos/*"],
      "@GConfigs/*": ["../../configs/*"],
      "@GEnvironment": ["../../scripts/environment"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}