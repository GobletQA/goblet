{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@GBE/Configs": ["configs"],
      "@GBE/Libs": ["src/libs"],
      "@GBE/Utils": ["src/utils"],
      "@GBE/Sockr": ["src/sockr"],
      "@GBE/Endpoints": ["src/endpoints"],
      "@GBE/Middleware": ["src/middleware"],
      "@GBE/Template": ["templates"],
      "@GBE": ["src"],
      "@GBE/Root": ["./"],
      "@gobletqa/shared": ["../shared/src"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@gobletqa/screencast": ["../screencast/src"],
      "@gobletqa/screencast/*": ["../screencast/src/*"],
      "@gobletqa/test-utils": ["../testUtils/src"],
      "@gobletqa/test-utils/*": ["../testUtils/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}