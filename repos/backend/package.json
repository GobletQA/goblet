{
  "name": "@gobletqa/backend",
  "version": "0.0.0",
  "description": "Goblet Backend API",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "pnpm build:docker",
    "build:docker": "node --enable-source-maps -r esbuild-register configs/build.config.ts",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "format": "pnpm lint && pnpm pretty",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "start": "DEBUG=\"http-proxy-middleware*\" node --enable-source-maps -r esbuild-register --max-http-header-size=262144 configs/dev.config.ts",
    "serve": "DEBUG=\"http-proxy-middleware*\" node --enable-source-maps -r esbuild-register dist/index.js >> /proc/1/fd/1",
    "dist": "node --enable-source-maps -r esbuild-register dist/index.js",
    "validate": "tsc --noEmit"
  },
  "optionalDependencies": {
    "dockerode": "3.3.2",
    "docker-events": "0.0.2"
  },
  "dependencies": {
    "@kubernetes/client-node": "0.18.1",
    "esbuild": "0.19.3",
    "esbuild-register": "3.5.0"
  },
  "devDependencies": {
    "@gobletqa/conductor": "workspace:*",
    "@gobletqa/environment": "workspace:*",
    "@gobletqa/logger": "workspace:*",
    "@gobletqa/repo": "workspace:*",
    "@gobletqa/shared": "workspace:*",
    "@gobletqa/workflows": "workspace:*",
    "@keg-hub/cli-utils": "0.9.0",
    "@keg-hub/jsutils": "9.10.0",
    "@keg-hub/parse-config": ">=2.1.0",
    "esbuild-plugin-path-alias": "1.0.7",
    "express": "4.18.2",
    "express-jwt": "8.4.1",
    "firebase-admin": "11.10.1",
    "get-port-please": "3.1.1",
    "http-proxy-middleware": "2.0.6",
    "jsonwebtoken": "9.0.2",
    "module-alias": "2.2.2",
    "@ltipton/esdev": "0.2.1",
    "@types/debug": "4.1.9",
    "@types/express": "4.17.18",
    "@types/node": "20.7.1",
    "nodemon": "3.0.1"
  }
}
