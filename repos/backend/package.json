{
  "name": "@gobletqa/backend",
  "version": "0.0.0",
  "description": "Goblet Backend API",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "pnpm build:docker",
    "build:docker": "tsup --config ./configs/tsup.config.ts",
    "start": "DEBUG=\"http-proxy-middleware*\" tsup-node --config ./configs/tsup.config.ts --watch src --watch configs --watch ../conductor/src  --watch ../shared/src --onSuccess 'node --enable-source-maps -r esbuild-register dist/index.js'",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "depcheck": "depcheck --ignores=\"@GBE/*,@gobletqa/backend,esbuild*,express*\" --ignore-patterns=build,dist,.*,coverage,temp,*jest*",
    "format": "pnpm lint && pnpm pretty",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "serve": "DEBUG=\"http-proxy-middleware*\" node --enable-source-maps -r esbuild-register dist/index.js",
    "dist": "node --enable-source-maps -r esbuild-register dist/index.js",
    "validate": "tsc --noEmit"
  },
  "optionalDependencies": {
    "dockerode": "3.3.2",
    "docker-events": "0.0.2"
  },
  "dependencies": {
    "@gobletqa/configs": "workspace:*",
    "@gobletqa/conductor": "workspace:*",
    "@gobletqa/environment": "workspace:*",
    "@gobletqa/goblet": "workspace:*",
    "@gobletqa/logger": "workspace:*",
    "@gobletqa/shared": "workspace:*",
    "@gobletqa/workflows": "workspace:*",
    "@keg-hub/jsutils": "9.10.0",
    "@kubernetes/client-node": "0.20.0",
    "esbuild": "0.19.7",
    "esbuild-register": "3.5.0",
    "express": "4.18.2",
    "express-jwt": "8.4.1",
    "express-winston": "4.2.0",
    "express-rate-limit": "7.1.4",
    "firebase-admin": "11.11.0",
    "jsonwebtoken": "9.0.2",
    "module-alias": "2.2.3"
  },
  "devDependencies": {
    "@types/express": "4.17.18",
    "@types/node": "20.9.3",
    "tsup": "8.0.1"
  }
}
