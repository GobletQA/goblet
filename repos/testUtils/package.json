{
  "name": "@gobletqa/test-utils",
  "version": "0.0.0",
  "description": "Utility methods to help writing tests on with GobletQA",
  "main": "src/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "scripts": {
    "wip:build": "node configs/build.config.js",
    "clean": "yarn clean:dist; yarn clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "yarn clean; yarn install",
    "clean:nm": "rm -rf node_modules",
    "format": "yarn lint && yarn pretty",
    "json:rm": "dot-json package.json devDependencies -d",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w"
  },
  "dependencies": {
    "@gobletqa/screencast": "*",
    "@gobletqa/shared": "*",
    "@jest/create-cache-key-function": "*",
    "@keg-hub/cli-utils": ">=0.8.0-alpha1",
    "@keg-hub/jsutils": ">=9.6.1",
    "@keg-hub/parse-config": ">=2.1.0",
    "@ltipton/parkin": "3.0.0-beta",
    "esbuild": "*",
    "esbuild-plugin-path-alias": "*",
    "esbuild-register": "*",
    "expect": "*",
    "glob": "8.0.3",
    "jest": "*",
    "jest-html-reporter": "3.7.0",
    "jest-jasmine2": "*",
    "module-alias": "*",
    "playwright": "*",
    "ts-jest": "*"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/express": "4.17.9",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.0"
  }
}
