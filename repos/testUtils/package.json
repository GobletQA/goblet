{
  "name": "@gobletqa/test-utils",
  "version": "0.0.0",
  "description": "Utility methods to help writing tests on with GobletQA",
  "main": "index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "pnpm build:code && pnpm build:cfg && pnpm build:fix",
    "build:code": "node -r esbuild-register configs/build.config.ts",
    "build:cfg": "node -r esbuild-register configs/exam.cfg.build.ts",
    "build:fix": "node -r esbuild-register scripts/fixPaths.ts",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "format": "pnpm lint && pnpm pretty",
    "test:html": "node  -r esbuild-register src/exam/reporters/html/__tests__/generateHtml.ts",
    "ex:dev": "GOBLET_CONFIG_BASE=$(pwd)/src/exam/__tests__ EXAM_DEV_CLI=1 node --no-warnings --loader esbuild-register/loader -r esbuild-register ../exam/src/bin/exam.ts --config ../exam.config.ts --root $(pwd)/src/exam/__tests__",
    "exam": "node ../exam/.bin/exam.js --config ../exam.config.ts --root $(pwd)/src/exam/__tests__",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w"
  },
  "dependencies": {
    "@gobletqa/browser": "workspace:*",
    "@gobletqa/exam": "workspace:*",
    "@gobletqa/environment": "workspace:*",
    "@gobletqa/goblet": "workspace:*",
    "@gobletqa/logger": "workspace:*",
    "@gobletqa/repo": "workspace:*",
    "@gobletqa/shared": "workspace:*",
    "@keg-hub/jsutils": "9.10.0",
    "@ltipton/parkin": "6.3.1",
    "esbuild": "0.19.3",
    "esbuild-plugin-path-alias": "1.0.7",
    "esbuild-register": "3.5.0",
    "expect": "29.7.0",
    "glob": "10.3.10",
    "module-alias": "2.2.2",
    "playwright": "1.38.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.9",
    "@types/express": "4.17.18",
    "@types/node": "20.7.1"
  }
}
