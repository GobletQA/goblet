{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@gobletqa/race": ["src"],
      "@gobletqa/race/*": ["src/*"],
      "@GBR/actions": [ "src/actions" ],
      "@GBR/actions/*": [ "src/actions/*" ],
      "@GBR/components": [ "src/components" ],
      "@GBR/components/*": [ "src/components/*" ],
      "@GBR/configs": [ "configs" ],
      "@GBR/configs/*": [ "configs/*" ],
      "@GBR/constants": [ "src/constants" ],
      "@GBR/constants/*": [ "src/constants/*" ],
      "@GBR/contexts": [ "src/contexts" ],
      "@GBR/contexts/*": [ "src/contexts/*" ],
      "@GBR/hooks": [ "src/hooks" ],
      "@GBR/hooks/*": [ "src/hooks/*" ],
      "@GBR/services": [ "src/services" ],
      "@GBR/services/*": [ "src/services/*" ],
      "@GBR/types": [ "src/types" ],
      "@GBR/types/*": [ "src/types/*" ],
      "@GBR/utils": [ "src/utils" ],
      "@GBR/utils/*": [ "src/utils/*" ],
      "@gobletqa/components": ["../components/src"],
      "@gobletqa/components/*": ["../components/src/*"],
      "GobletRoot": ["../../"]
    }
  },
  "include": ["src"],
  "references": [{ "path": "tsconfig.vite.json" }]
}
