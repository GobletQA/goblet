{
  "name": "@gobletqa/race",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "yarn clean:dist; yarn clean:nm",
    "clean:dist": "rm -rf dist",
    "clean:full": "yarn clean; yarn install",
    "clean:nm": "rm -rf node_modules",
    "build": "GOBLET_ENV=production vite build --clearScreen false -c configs/vite.config.ts",
    "prod": "GOBLET_ENV=production vite --clearScreen false -c configs/vite.config.ts",
    "serve": "serve -p 19006 dist",
    "start": "vite --clearScreen false -c configs/vite.config.ts --force"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@gobletqa/shared": "*",
    "@keg-hub/cli-utils": "*",
    "@keg-hub/jsutils": "*",
    "@mui/icons-material": "5.10.3",
    "@mui/material": "5.10.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@mui/x-date-pickers": "5.0.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "esbuild-register": "3.3.3",
    "serve": "14.1.2",
    "vite": "3.1.4",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-svgr": "2.2.1",
    "vite-plugin-svgr-component": "1.0.1",
    "vite-tsconfig-paths": "3.5.0"
  }
}
