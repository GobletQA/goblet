{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "typeRoots": [
      "./src/types",
      "../shared/src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@GWF/git": [ "src/git" ],
      "@GWF": ["src"],
      "@GWFRoot": ["./."],
      "@GConfigs/*": ["../../configs/*"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@gobletqa/shared": ["../shared/src"],
      "@gobletqa/workflows/types": ["src/types"],
      "@gobletqa/workflows/types/*": ["src/types/*"],
      "@gobletqa/workflows": ["src"],
      "@gobletqa/workflows/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}