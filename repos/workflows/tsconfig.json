{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@GWF/git": [ "src/git" ],
      "@GWF": ["src"],
      "@GWFRoot": ["./."],
      "@GConfigs/*": ["../../configs/*"],
      "@gobletqa/workflows/types": ["src/types"],
      "@gobletqa/workflows/types/*": ["src/types/*"],
      "@gobletqa/workflows": ["src"],
      "@gobletqa/workflows/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}