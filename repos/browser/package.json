{
  "name": "@gobletqa/browser",
  "version": "0.0.0",
  "description": "Encrypt and decrypt variables",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "ISC",
  "keywords": [],
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "pnpm b:build && pnpm types",
    "b:build": "node --enable-source-maps -r esbuild-register configs/build.config.ts",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "format": "pnpm lint && pnpm pretty",
    "json:rm": "dot-json package.json devDependencies -d",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "test": "jest --colors --config ./configs/jest.config.js",
    "test:clean": "jest --colors --config ./configs/jest.config.js --clearCache",
    "types": "tsc --emitDeclarationOnly --outDir dist",
    "validate": "tsc --noEmit"
  },
  "dependencies": {
    "@gobletqa/environment": "workspace:../environment",
    "@keg-hub/jsutils": ">=9.7.1",
    "@keg-hub/parse-config": ">=2.1.0",
    "@keg-hub/spawn-cmd": ">=0.3.2",
    "playwright": "1.36.1"
  },
  "devDependencies": {
    "@ltipton/esdev": "0.2.1",
    "@types/debug": "4.1.5",
    "@types/dockerode": "3.3.9",
    "@types/express": "4.17.17",
    "@types/jest": "29.1.2",
    "@types/node": "18.15.13",
    "jest": "29.5.0",
    "esbuild": "0.18.17",
    "esbuild-plugin-path-alias": "1.0.7",
    "esbuild-register": "3.4.2",
    "ghost-cursor-pw": "1.1.18",
    "tail-file": "1.4.15"
  }
}

