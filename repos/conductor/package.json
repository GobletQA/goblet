{
  "name": "@gobletqa/conductor",
  "version": "0.0.0",
  "description": "Goblet Conductor API",
  "main": "dist/index.ts",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "yarn build:docker",
    "build:docker": "node --enable-source-maps -r esbuild-register configs/build.config.ts",
    "cd:build": "node configs/build.config.js && tsc --emitDeclarationOnly --outDir dist",
    "clean": "yarn clean:dist; yarn clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "yarn clean; yarn install",
    "clean:nm": "rm -rf node_modules",
    "format": "yarn lint && yarn pretty",
    "json:rm": "dot-json package.json devDependencies -d",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "types": "tsc --emitDeclarationOnly --outDir dist",
    "validate": "tsc --noEmit"
  },
  "dependencies": {
    "@gobletqa/shared": "*",
    "@keg-hub/cli-utils": "0.8.0-alpha",
    "@keg-hub/jsutils": "*",
    "@keg-hub/parse-config": "*",
    "@kubernetes/client-node": "*",
    "axios": "*",
    "express": "*",
    "express-async-handler": "1.2.0",
    "get-port-please": "2.5.0",
    "http-proxy-middleware": "*",
    "jsonwebtoken": "*",
    "module-alias": "*",
    "source-map-support": "*"
  },
  "devDependencies": {
    "@ltipton/esdev": "*",
    "@types/debug": "4.1.5",
    "@types/dockerode": "3.3.9",
    "@types/express": "4.17.9",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.0",
    "docker-events": "0.0.2",
    "dockerode": "3.3.2",
    "esbuild": "*",
    "esbuild-plugin-path-alias": "*",
    "esbuild-register": "*",
    "jest": "*",
    "nodemon": "*"
  }
}
