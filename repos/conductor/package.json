{
  "name": "@gobletqa/conductor",
  "version": "0.0.0",
  "description": "Goblet Conductor API",
  "main": "dist",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "pnpm build:docker",
    "build:docker": "node --enable-source-maps -r esbuild-register configs/build.config.ts",
    "cd:build": "node configs/build.config.js && tsc --emitDeclarationOnly --outDir dist",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "format": "pnpm lint && pnpm pretty",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "types": "tsc --emitDeclarationOnly --outDir dist",
    "validate": "tsc --noEmit"
  },
  "peerDependencies": {
    "@keg-hub/cli-utils": "0.9.0",
    "@keg-hub/jsutils": "9.8.3",
    "@keg-hub/parse-config": ">=2.1.0",
    "@gobletqa/environment": "workspace:*",
    "@gobletqa/logger": "workspace:*",
    "@gobletqa/shared": "workspace:*"
  },
  "optionalDependencies": {
    "@kubernetes/client-node": "0.18.1",
    "dockerode": "3.3.2",
    "docker-events": "0.0.2"
  },
  "dependencies": {
    "express": "4.18.2",
    "get-port-please": "2.5.0",
    "http-proxy-middleware": "2.0.6"
  },
  "devDependencies": {
    "@ltipton/esdev": "0.2.1",
    "@types/debug": "4.1.5",
    "@types/dockerode": "3.3.9",
    "@types/express": "4.17.17",
    "@types/jest": "29.1.2",
    "@types/node": "18.15.13",
    "esbuild": "0.19.2",
    "esbuild-register": "3.4.2",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "source-map-support": "0.5.21"
  }
}
