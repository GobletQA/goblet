{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "typeRoots": [
      "./src/types",
      "../shared/src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@GCD/configs": ["configs"],
      "@GCD/configs/*": ["configs/*"],
      "@GCD": ["src"],
      "@GCD/*": ["src/*"],
      "@gobletqa/shared/utils/loadEnvs": ["../shared/src/utils/loadEnvs"],
      "@gobletqa/shared/utils/getOrigin": ["../shared/src/utils/getOrigin"],
      "@gobletqa/shared": ["../shared/src"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@gobletqa/conductor": ["src"],
      "@gobletqa/conductor/*": ["src/*"],
      "@gobletqa/conductor/root": ["./"],
      "@GLogs/*": ["../../logs/*"],
      "@GRepos/*": ["../../repos/*"],
      "@GConfigs/*": ["../../configs/*"],
      "@GEnvironment": ["../../scripts/environment"]
    }
  },
  "exclude": ["node_modules", "dist"]
}