{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "typeRoots": [
      "./src/types",
      "../shared/src/types",
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@GSC/Configs": ["configs"],
      "@GSC/Configs/*": ["configs/*"],
      "@GSC/Scripts": ["scripts"],
      "@GSC/Scripts/*": ["scripts/*"],
      "@GSC": ["src"],
      "@GSC/*": ["src/*"],
      "@GSCRoot": ["./"],
      "@GSCRoot/*": ["./*"],
      "@gobletqa/shared": ["../shared/src"],
      "@gobletqa/shared/*": ["../shared/src/*"],
      "@gobletqa/screencast": ["src"],
      "@gobletqa/screencast/*": ["src/*"],
      "@GLogs/*": ["../../logs/*"],
      "@GRepos/*": ["../../repos/*"],
      "@GConfigs/*": ["../../configs/*"],
      "@GEnvironment": ["../../scripts/environment"]
    }
  },
  "include": [
    "src",
    "configs",
    "scripts",
    "../shared",
    "../testUtils",
    "../workflows"
  ],
  "exclude": ["node_modules", "dist"]
}