{
  "name": "@gobletqa/screencast",
  "version": "0.0.0",
  "description": "VNC Screencast Backend API and Servers",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "build": "yarn build:docker",
    "build:docker": "yarn sc:build && yarn vnc:build",
    "clean:install": "yarn clean:full; yarn install",
    "clean:full": "yarn clean; yarn clean:nm; yarn install",
    "clean:nm": "rm -rf node_modules",
    "format": "yarn lint && yarn pretty",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "sc:build": "node configs/build.config.js",
    "sc:serve": "node dist/index.js",
    "serve": "yarn vnc:serve && yarn sc:serve",
    "start": "DEV_BUILD=1 node configs/build.config.js",
    "vnc": "node dist/vnc.js",
    "vnc:build": "node configs/vnc.build.config.js",
    "vnc:start": "DEV_BUILD=1 node configs/vnc.build.config.js",
    "vnc:serve": "pm2 start configs/vnc.pm2.config.js"
  },
  "resolutions": {
    "@keg-hub/args-parse": "9.0.0",
    "@keg-hub/cli-utils": "0.7.0",
    "@keg-hub/jsutils": "9.3.5",
    "@keg-hub/parse-config": "2.1.0",
    "@keg-hub/spawn-cmd": "0.3.2",
    "@ltipton/parkin": "1.4.0",
    "esbuild-register": "3.3.3",
    "mime": "3.0.0",
    "nodemon": "2.0.19",
    "playwright": "1.27.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@gobletqa/shared": "*",
    "@jest/expect-utils": "28.1.3",
    "@keg-hub/cli-utils": "*",
    "@keg-hub/jsutils": "*",
    "@keg-hub/parse-config": "*",
    "@keg-hub/spawn-cmd": "*",
    "@ltipton/parkin": "*",
    "expect": "28.1.0",
    "express": "4.17.3",
    "fs-extra": "10.1.0",
    "jest": "29.2.0",
    "module-alias": "2.2.2",
    "playwright": "1.27.1",
    "socket.io": "4.5.4",
    "source-map-support": "0.5.21",
    "tail-file": "1.4.15",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*",
    "vm2": "3.9.9"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/express": "4.17.9",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.0",
    "esbuild": "0.15.3",
    "esbuild-plugin-path-alias": "1.0.6",
    "nodemon": "*"
  }
}
