{
  "name": "@gobletqa/screencast",
  "version": "0.0.0",
  "description": "VNC Screencast Backend API and Servers",
  "main": "index.js",
  "author": "Lance Tipton",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "url": "git+https://github.com/GobletQA/goblet.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/GobletQA/goblet/issues"
  },
  "scripts": {
    "bs:build": "node -r esbuild-register configs/bs.build.config.ts",
    "bs:start": "node -r esbuild-register configs/bs.build.config.ts",
    "build": "yarn build:docker",
    "build:docker": "yarn sc:build && yarn vnc:build  && yarn vs:build",
    "clean": "yarn clean:dist; yarn clean:nm",
    "clean:dist": "rm -rf dist && mkdir dist && echo \"\" >> dist/.keep",
    "clean:full": "yarn clean; yarn install",
    "clean:nm": "rm -rf node_modules",
    "format": "yarn lint && yarn pretty",
    "json:rm": "dot-json package.json devDependencies -d",
    "lint": "eslint --color --cache --fix --quiet --ignore-path ../../.eslintignore --config ./configs/eslintrc.config.js",
    "pretty": "prettier --ignore-path ../../.prettierignore --config ./configs/prettier.config.js -w",
    "sc:build": "node -r esbuild-register configs/build.config.js",
    "serve": "/bin/bash scripts/initialize.sh",
    "start": "DEV_BUILD=1 node -r esbuild-register configs/build.config.js",
    "vnc": "node -r esbuild-register dist/vnc.js",
    "vnc:build": "node -r esbuild-register configs/vnc.build.config.js",
    "vnc:start": "DEV_BUILD=1 node -r esbuild-register configs/vnc.build.config.js"
  },
  "dependencies": {
    "@gobletqa/shared": "*",
    "@keg-hub/jsutils": "*",
    "@keg-hub/parse-config": "*",
    "@keg-hub/spawn-cmd": "*",
    "@ltipton/parkin": "*",
    "expect": "*",
    "express": "*",
    "fs-extra": "10.1.0",
    "playwright": "*",
    "socket.io": "*",
    "source-map-support": "*",
    "tail-file": "1.4.15"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/express": "4.17.9",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.0",
    "esbuild": "*",
    "esbuild-plugin-path-alias": "*",
    "esbuild-register": "*",
    "nodemon": "*"
  }
}
