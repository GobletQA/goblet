{
  "name": "@gobletqa/components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/gobletComponents.js",
      "require": "./dist/gobletComponents.umd.cjs"
    }
  },
  "main": "dist/gobletComponents.umd.cjs",
  "module": "dist/gobletComponents.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "build": "GOBLET_ENV=production vite build --clearScreen false -c configs/vite.config.ts",
    "prod": "GOBLET_ENV=production vite --clearScreen false -c configs/vite.config.ts",
    "serve": "serve -p 19006 dist",
    "start": "vite --clearScreen false -c configs/vite.config.ts --force"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@keg-hub/jsutils": "9.10.0",
    "@mui/icons-material": "5.14.11",
    "@mui/lab": "5.0.0-alpha.146",
    "@mui/material": "5.14.11",
    "react-selecto": "1.26.0"
  },
  "devDependencies": {
    "@gobletqa/shared": "workspace:*",
    "@keg-hub/cli-utils": "0.9.0",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@vitejs/plugin-react-swc": "3.4.0",
    "esbuild": "0.19.3",
    "esbuild-register": "3.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "serve": "14.2.1",
    "vite": "4.4.9",
    "vite-plugin-svgr-component": "1.0.1",
    "vite-tsconfig-paths": "4.2.1"
  }
}
