{
  "name": "@gobletqa/vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "GOBLET_ENV=production NODE_ENV=production vite build --clearScreen false -c configs/vite.config.ts",
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "depcheck": "depcheck --ignores=\"@*/*,@GobletRoot,@gobletqa/vite\" --ignore-patterns=build,dist,.*,coverage,temp,*jest*",
    "prod": "GOBLET_ENV=production NODE_ENV=production vite --clearScreen false -c configs/vite.config.ts",
    "cache": "GOBLET_ENV=local NODE_ENV=local vite --force --clearScreen false -c configs/vite.config.ts",
    "serve": "GOBLET_ENV=production NODE_ENV=production serve -p 19006 dist",
    "start": "VITE_CJS_TRACE=true GOBLET_ENV=local NODE_ENV=local vite --clearScreen false -c configs/vite.config.ts"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@gobletqa/shared": "workspace:*",
    "@gobletqa/race": "workspace:*",
    "@gobletqa/monaco": "workspace:*",
    "@keg-hub/cli-utils": "0.9.0",
    "@keg-hub/jsutils": "10.0.0",
    "@ltipton/parkin": "6.6.1",
    "@monaco-editor/react": "4.6.0",
    "@mui/icons-material": "5.14.18",
    "@mui/material": "5.14.18",
    "@novnc/novnc": "1.4.0",
    "@redux-devtools/extension": "3.2.6",
    "@reduxjs/toolkit": "1.9.7",
    "allotment": "1.19.3",
    "axios": "1.6.2",
    "comlink": "4.4.1",
    "firebase": "10.6.0",
    "million": "2.6.4",
    "oidc-react": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.19.0",
    "react-error-boundary": "4.0.12",
    "redux": "4.2.1",
    "socket.io-client": "4.7.2"
  },
  "devDependencies": {
    "@gobletqa/components": "workspace:*",
    "@gobletqa/configs": "workspace:*",
    "@mui/lab": "5.0.0-alpha.153",
    "@types/node": "20.9.3",
    "@types/novnc__novnc": "1.3.4",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.16",
    "@vitejs/plugin-react-swc": "3.5.0",
    "esbuild-register": "3.5.0",
    "serve": "14.2.1",
    "vite": "5.0.2",
    "vite-plugin-comlink": "3.0.5",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-svgr-component": "1.0.1",
    "vite-tsconfig-paths": "4.2.1"
  }
}
