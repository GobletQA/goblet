    "@monaco-editor/react": "4.3.1",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "@novnc/novnc": "1.3.0",
    "@redux-devtools/extension": "3.2.3",
    "@reduxjs/toolkit": "1.8.5",
    "axios": "0.27.2",
    "firebase": "9.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.37.0",
    "react-hook-form-mui": "5.7.2",
