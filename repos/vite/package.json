    "@keg-hub/cli-utils": "0.9.0",
    "@keg-hub/jsutils": "9.8.3",
    "@monaco-editor/react": "4.5.1",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.3",
    "@novnc/novnc": "1.4.0",
    "@redux-devtools/extension": "3.2.5",
    "@reduxjs/toolkit": "1.9.5",
    "allotment": "1.19.0",
    "appwrite": "11.0.0",
    "axios": "1.4.0",
    "comlink": "4.4.1",
    "firebase": "9.12.1",
    "markerjs2": "2.28.1",
    "oidc-react": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-idle-timer": "5.6.2",
    "react-redux": "8.0.5",
    "react-router-dom": "6.11.1",
    "react-selecto": "1.22.2",
    "redux": "4.2.1",
    "socket.io-client": "4.6.1"
