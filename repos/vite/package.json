{
  "name": "@gobletqa/vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "build": "GOBLET_ENV=production NODE_ENV=production vite build --clearScreen false -c configs/vite.config.ts",
    "prod": "GOBLET_ENV=production NODE_ENV=production vite --clearScreen false -c configs/vite.config.ts",
    "cache": "GOBLET_ENV=local NODE_ENV=local vite --force --clearScreen false -c configs/vite.config.ts",
    "serve": "GOBLET_ENV=production NODE_ENV=production serve -p 19006 dist",
    "start": "GOBLET_ENV=local NODE_ENV=local vite --clearScreen false -c configs/vite.config.ts"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@gobletqa/shared": "workspace:*",
    "@keg-hub/cli-utils": "0.9.0",
    "@keg-hub/jsutils": "9.8.3",
    "@monaco-editor/react": "4.5.2",
    "@mui/icons-material": "5.14.11",
    "@mui/material": "5.14.11",
    "@novnc/novnc": "1.4.0",
    "@redux-devtools/extension": "3.2.5",
    "@reduxjs/toolkit": "1.9.6",
    "allotment": "1.19.3",
    "appwrite": "13.0.0",
    "axios": "1.5.1",
    "comlink": "4.4.1",
    "firebase": "10.4.0",
    "markerjs2": "2.31.0",
    "oidc-react": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-idle-timer": "5.7.2",
    "react-redux": "8.1.2",
    "react-router-dom": "6.16.0",
    "react-selecto": "1.26.0",
    "redux": "4.2.1",
    "socket.io-client": "4.7.2"
  },
  "devDependencies": {
    "@gobletqa/components": "workspace:*",
    "@mui/lab": "5.0.0-alpha.146",
    "@types/node": "20.7.1",
    "@types/novnc__novnc": "1.3.0",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@vitejs/plugin-react-swc": "3.4.0",
    "esbuild-register": "3.5.0",
    "serve": "14.2.1",
    "vite": "4.4.9",
    "vite-plugin-comlink": "3.0.5",
    "vite-plugin-monaco-editor": "1.1.0",
    "vite-plugin-svgr-component": "1.0.1",
    "vite-tsconfig-paths": "4.2.1"
  }
}
