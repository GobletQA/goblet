{
  "name": "@gobletqa/vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "pnpm clean:dist; pnpm clean:nm",
    "clean:dist": "rm -rf dist",
    "clean:full": "pnpm clean; pnpm install",
    "clean:nm": "rm -rf node_modules",
    "build": "GOBLET_ENV=production NODE_ENV=production vite build --clearScreen false -c configs/vite.config.ts",
    "prod": "GOBLET_ENV=production NODE_ENV=production vite --clearScreen false -c configs/vite.config.ts",
    "cache": "GOBLET_ENV=local NODE_ENV=local vite --force --clearScreen false -c configs/vite.config.ts",
    "serve": "GOBLET_ENV=production NODE_ENV=production serve -p 19006 dist",
    "start": "GOBLET_ENV=local NODE_ENV=local vite --clearScreen false -c configs/vite.config.ts"
  },
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@gitbeaker/browser": "35.8.1",
